<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	<bean id="address" class="com.demo.autowiring.Address">
		<property name="city" value="Kanpur"></property>
		<property name="street" value="Street One"></property>
	</bean>
	
	<bean id="emp1" class="com.demo.autowiring.Employee" autowire="constructor">
	</bean>



</beans>